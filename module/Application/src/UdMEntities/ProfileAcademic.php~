<?php



use Doctrine\ORM\Mapping as ORM;

/**
 * ProfileAcademic
 *
 * @ORM\Table(name="profile_academic")
 * @ORM\Entity
 */
class ProfileAcademic
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var float
     *
     * @ORM\Column(name="minval", type="float", precision=10, scale=0, nullable=false)
     */
    private $minval;

    /**
     * @var float
     *
     * @ORM\Column(name="maxval", type="float", precision=10, scale=0, nullable=false)
     */
    private $maxval;

    /**
     * @var string
     *
     * @ORM\Column(name="grade", type="string", length=45, nullable=false)
     */
    private $grade;


}
